{
  "name": "dns-lookup.com",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"no test specified\"",
    "start": "pm2 startOrRestart ecosystem.json --env production --watch",
    "deploy": "pm2 deploy ecosystem.json production",
    "travis-deploy": "if-env TRAVIS_BRANCH=master TRAVIS_PULL_REQUEST=false && npm run semantic-release || echo 'Deploy is not run'",
    "check-updates": "ncu",
    "upgrade": "npm run check-updates -- --upgradeAll && npm install",
    "develop": "PORT=5002 nodemon app",
    "semantic-release": "semantic-release pre && git stash && ssh-keyscan 192.241.157.86 >> ~/.ssh/known_hosts  && npm run deploy && git stash apply --index && semantic-release post"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ]
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "release": {
    "getLastRelease": "last-release-git"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/circlecell/dns-lookup.com.git"
  },
  "keywords": [
    "dns",
    "lookup"
  ],
  "author": "circlecell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/circlecell/dns-lookup.com/issues"
  },
  "homepage": "https://github.com/circlecell/dns-lookup.com#readme",
  "devDependencies": {
    "ghooks": "^1.3.2",
    "if-env": "^1.0.0",
    "last-release-git": "0.0.1",
    "nodemon": "^1.11.0",
    "npm-check-updates": "^2.8.5",
    "pm2": "^2.0.18",
    "semantic-release": "^4.3.5",
    "validate-commit-msg": "^2.8.2"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "co-express": "^1.2.2",
    "deep-equal": "^1.0.1",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "mongoose": "^4.6.4",
    "native-dns": "^0.7.0"
  }
}
